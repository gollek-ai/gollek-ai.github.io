---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0">
  <channel>
    <title>Gollek Blog</title>
    <link>{{ '/blog/' | relative_url }}</link>
    <description>Gollek release notes, architecture updates, and developer guides.</description>
    <language>en-us</language>
    <lastBuildDate>{{ "now" | date_to_rfc822 }}</lastBuildDate>
    {% assign posts = site.pages | where_exp: "p", "p.url contains '/blog/' and p.url != '/blog/' and p.title and p.date" | sort: "date" | reverse %}
    {% for post in posts limit: 20 %}
    <item>
      <title>{{ post.title | xml_escape }}</title>
      <link>{{ post.url | relative_url }}</link>
      <guid>{{ post.url | relative_url }}</guid>
      <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
      <description>{{ post.description | default: post.title | xml_escape }}</description>
    </item>
    {% endfor %}
  </channel>
</rss>
